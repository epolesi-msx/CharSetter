10 '-----------------------------------
20 ' SHOWCSET v1.0
30 ' SHOW MSX CHARACTER SET
40 ' By Emerson Polesi on 14/06/2020
50 '-----------------------------------
60 FG=PEEK(&HF3E9):BG=PEEK(&HF3EA):SC=PEEK(&HFCAF)
70 COLOR 1,15,15:SCREEN 2
80 FOR X=0 TO 16
90 LINE(X*11+37,5)-(X*11+37,181),1
100 LINE(37,X*11+5)-(213,X*11+5),1
110 NEXT X
120 COLOR 1:OPEN "GRP:" FOR OUTPUT AS#1
130 FOR Y=0 TO 3:FOR X=0 TO 15
140 PSET (X*11+39,Y*11+7),15:IF (Y<2) THEN PRINT #1,CHR$(1)+CHR$(Y*16+64+X); ELSE PRINT #1,CHR$(Y*16+X);
150 PSET (X*11+39,(Y+4)*11+7),15:PRINT #1,CHR$(Y*16+X+64);
160 PSET (X*11+39,(Y+8)*11+7),15:PRINT #1,CHR$(Y*16+X+128);
170 PSET (X*11+39,(Y+12)*11+7),15:PRINT #1,CHR$(Y*16+X+192);
180 NEXT X:NEXT Y
190 A$=INPUT$(1)
200 COLOR FG,BG,BD:SCREEN SC
